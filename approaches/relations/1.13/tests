# tests

# <syntax>
	<rule-pre1>.
	<rule-pre2>.
	# should imply
	<rule-result>?

	# blank line as delimiter

	<rules-pre1>.
	<rules-pre2>.
	# should NOT imply
	<rule-result>!?

	# y: this works with the current laws
# </syntax>


{ a { b (p <a> <b>)}}.
{ a (p <a> <a>)}? # y

{ a { b (p <a> <b>)}}.
{ a { b (p <b> <a>)}}? # y

{ a { b (p <a> <b>)}}.
{ b { a (p <a> <b>)}}? # y

implies { a { b (p <a> <b>)}} { a (p <a> <a>)}?

{ a (= <a> <a>)}? # y

= 'false' { a (= <a> any_cell)}?

= 'false' { a (= 'false' (= <a> any_cell))}?

p x.
= 'false' { a (= 'false' (p <a>))}?

= 'false' (p x).
= 'false' { a (p <a>)}?

= 'false' (= (p x) (p y)).
= 'false' (= x y)?

= 'false' { a (= <a> any_cell) }.
= 'false' (= a_cell any_cell)!? # y

{ a (= 'false' { b (= <b> any_cell)})}.
{ a (= 'false' (= <a> any_cell))}!?

{ a (= (= 'false' (= <a> <a>)) { b (= <b> any_cell)})}.
{ a (= (= 'false' (= <a> <a>)) (= <a> any_cell))}!?
