# contraposition

{0 {1
	(= 
		(=> 0 1)
		(=> (= 'false' (= 'true' 1)) (= 'false' (= 'true' 0)))
	)
}}?

{0 {1
	(= 
		(=> 0 1)
		(=> 0 1)
	)
}}.

# b11
{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> 0 1)
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> 'true' 'true')
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				'true'
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> 'false' 'false')
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' 'true') (= 'false' 'true')
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' (= 'true' 'true')) (= 'false' (= 'true' 'true'))
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'true' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' (= 'true' 1)) (= 'false' (= 'true' 0))
			)
		)
	)
}}. # did

# b12
{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> 0 1)
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> 'true' 'false')
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' 'false') (= 'false' 'true'))
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' 'false') (= 'false' 'true'))
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' (= 'true' 'false')) (= 'false' (= 'true' 'true')))
			)
		)
	)
}}.

{0 {1
	(=> (= 'true' 0)
		(=> (= 'false' 1)
			(= 
				(=> 0 1)
				(=> (= 'false' (= 'true' 1)) (= 'false' (= 'true' 0)))
			)
		)
	)
}}.

# with b11 and b12 we get:

{0 {1
	(=> (= 'true' 0)
		(= 
			(=> 0 1)
			(=> (= 'false' (= 'true' 1)) (= 'false' (= 'true' 0)))
		)
	)
}}.

