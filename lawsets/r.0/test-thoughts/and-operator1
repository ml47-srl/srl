# proof by cases: done

{0 {1
	(=
		(and (= 'true' 0) (= 'true' 1))
		(and (= 'true' 1) (= 'true' 0))
	)
}}.

{0 {1
	(=
		(and 0 1)
		(and (= 'true' 1) (= 'true' 0))
	)
}}.

{0 {1
	(=
		(and 1 0)
		(and (= 'true' 0) (= 'true' 1))
	)
}}.

{0 {1
	(=
		(and 1 0)
		(and 0 1)
	)
}}.

{0 {1
	(=
		(and 0 1)
		(and 1 0)
	)
}}.

{0 {1
	(=
		(and 0 1)
		(and 1 0)
	)
}}. # did
