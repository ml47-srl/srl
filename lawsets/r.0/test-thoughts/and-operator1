# and-operator1

and 'true' 'true'!
= 'false' (and 'false' 'true')!
= 'false' (and 'true' 'false')!
= 'false' (and 'false' 'false')!

{0 {1
	(=
		(and 0 1)
		(and (= 'true' 0) (= 'true' 1))
	)
}}!

# cd1
{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'true' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'true' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and 'true' 'true')
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'true' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and (= 'true' 1) (= 'true' 0))
			]
		]
	)
}}.

# cd2
{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'false' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'false' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and 'true' 'false')
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'false' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				'false'
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'false' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and 'false' 'true')
			]
		]
	)
}}.

{0 {1
	(=
		[=> (= 'true' (= 'true' 0))
			[=> (= 'false' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and (= 'true' 1) (= 'true' 0))
			]
		]
	)
}}.

# cd3
{0 {1
	(=
		[=> (= 'false' (= 'true' 0))
			[=> (= 'true' (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
				(and (= 'true' 0) (= 'true' 1))
			]
		]
	)
}}.

