# scope-negation-without-impl

= 'false' {0 (= 'false' {1 (p 0 1)})}.
# ==>
= 'false' {0 (= 'false' (= 'true' (p 0 'fish')))}?

# 1
[=>
	(= 'false' {0 (= 'false' (= 'true' (p 0 'fish')))})
	(= 'false' {1 (= 'false' (= 'true' (p 1 'fish')))})
].

# 2
[=>
	{0 (= 'false' (= 'true' (p 0 'fish')))}
	{1 (= 'false' (= 'true' (p 1 'fish')))}
].

# NR:
= 'false' {0 (= 'false' {1 (p 0 1)})}.
{0 (= 'false' (= 'true' (p 0 'fish')))}.

# Ansatz f√ºr Zwischenschritt
= 'false' {0 (= 'false' (p 0 'fish'))}?
# 

'false'?
